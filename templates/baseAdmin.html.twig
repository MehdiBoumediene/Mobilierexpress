				<div class="toast toast-success" aria-live="polite">
					<div class="toast-progress" style="width: 0.18%;"></div>
					<button type="button" class="toast-close-button" role="button">×</button>
					<div class="toast-message">{{message}}</div>
				</div>
			</div>


		{% endfor %}
